public class MyBeanTest {
	public static void main(String[] args) {
		MyBeanStyleA obj = new MyBeanStyleA();
		run(obj);
		show(obj);
	}
	public static void run(MyBeanStyleA obj) {
		System.out.println("**************************");
		obj.testHello("Çöºó");
		obj.testHello("Çöºó");
		System.out.println("**************************");
	}
	public static void show(MyBeanStyleA obj) {
		System.out.println("**************************");
		obj.testHello("Çöºó");
		obj.testHello("Çöºó");
		System.out.println("**************************");
	}
}
